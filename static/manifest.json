{
  "name": "Gen GPT Reply",
  "version": "0.1",
  "description": "Generate Email using Open AI",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://mail.google.com/"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}