{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0",
  "description": "A simple Chrome extension with a popup.",
  "permissions": ["activeTab","storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images.png",
      "48": "images.png",
      "128": "images.png"
    }
  },
  "icons": {
    "16": "images.png",
    "48": "images.png",
    "128": "images.png"
  }
}